
<div class="grid">
  <!-- .grid-sizer empty element, only used for element sizing -->
	<div class="grid-sizer"></div>
	{#products.data}

		<a href="/product/{id}"><div class="grid-item"  data-id="{id}" data-cost="{price}">
			<div class="grid-contain">
				<img class="grid-image" src="{image.outfit}" data-cost="{price}">
				<strong><div>{designer}</div></strong>
				<div>{name}</div>
				<div class="price">{price}</div>
			</div>

		</div></a>
	{/products.data}


</div>
